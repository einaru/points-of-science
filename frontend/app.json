{
  "expo": {
    "name": "Science App",
    "slug": "msc-science-app",
    "version": "1.0.0",
    "sdkVersion": "44.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#6200ee",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "no.uvslokk.msc.ScienceApp",
      "supportsTablet": true
    },
    "android": {
      "package": "no.uvslokk.msc.ScienceApp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
