{
  "expo": {
    "name": "Points of Science",
    "slug": "msc-science-app",
    "version": "1.0.0",
    "sdkVersion": "44.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#6200ee",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#6200ee"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*.png",
      "src/shared/assets/**/*.png"
    ],
    "ios": {
      "bundleIdentifier": "no.uvslokk.msc.ScienceApp",
      "supportsTablet": false,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "no.uvslokk.msc.ScienceApp",
      "icon": "./assets/legacy-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6200ee"
      }
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#6200ee"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
